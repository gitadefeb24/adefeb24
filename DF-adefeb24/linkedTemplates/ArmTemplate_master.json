					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_KV_SQL_connectionString": {
						"value": "[parameters('LS_KV_SQL_connectionString')]"
					},
					"LS_KV_ADLS_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('LS_KV_ADLS_properties_typeProperties_connectionString_secretName')]"
					},
					"EventTrigger_properties_typeProperties_scope": {
						"value": "[parameters('EventTrigger_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DF-adefeb24_ArmTemplate_0')]"
			]
		},
		{
			"name": "DF-adefeb24_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
