				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"tumblingtrigger_properties_pipeline_parameters_WindowStartDate": {
						"value": "[parameters('tumblingtrigger_properties_pipeline_parameters_WindowStartDate')]"
					},
					"tumblingtrigger_properties_pipeline_parameters_WindowEndDate": {
						"value": "[parameters('tumblingtrigger_properties_pipeline_parameters_WindowEndDate')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DF-adefeb24_ArmTemplate_1')]"
			]
		}
	]
}